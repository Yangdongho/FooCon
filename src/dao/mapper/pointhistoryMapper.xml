<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="dao.PointHistoryDao">
	
	<insert id="insertHistory" parameterType="map">
		insert into pointHistory values('p'||seq_point.NEXTVAL,#{memberNum},#{brandNum},#{reviewnum},#{orderNum},'PLUS',50,sysdate,null,null)
	</insert>

</mapper>



